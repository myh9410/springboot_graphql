type Member {
    no: ID
    userId: String
    joinDate: String
    active: Active
    orders: [Order]
}

input MemberInput {
    userId: String
    active: Active
}

extend type Query {
    getMemberByNo(no: ID): Member
    getAllMembers: [Member]
}

extend type Mutation {
    saveMember(input: MemberInput): ID
    putMember(input: MemberInput): ID
    deleteMember(input: MemberInput): Boolean
}

enum Active {
    Y
    N
}